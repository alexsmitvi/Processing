int x=250;
int y=250;
void setup() {
  size(500,500);
}
void draw() {
  background(255);
  if (keyPressed && (key == CODED)) {
  if (keyCode ==LEFT) {
    x--;
  }else if (keyCode ==RIGHT){
    x++;
 }
 }
 if (keyPressed && (key == CODED)) {
  if (keyCode ==UP) {
    y--;
  }else if (keyCode ==DOWN){
    y++;
 }
 }
  rect(x,y,50,50);
}
